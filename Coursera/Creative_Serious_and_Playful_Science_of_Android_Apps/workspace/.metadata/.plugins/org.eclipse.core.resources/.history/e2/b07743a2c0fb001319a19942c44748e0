
public class Test {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		System.out.println("Test Java");
		int N=1000;
		int sum = 0;
		System.out.println("N = " + 3*N*N*N);
		for (int i = 3*N*N*N; i > 1; i = i/2)
		{
			System.out.println(" i = " + i);
		    sum++;
		}
		System.out.println("sum = " + sum);
		
	}

}
